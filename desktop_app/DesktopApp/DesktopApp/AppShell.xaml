<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DesktopApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DesktopApp"
    Shell.FlyoutBehavior="Flyout"
    Title="DesktopApp">



    <ShellContent
        Title="UploadPage"
        ContentTemplate="{DataTemplate local:UploadPage}"
        Route="UploadPage" 
        x:Name="UploadPageShell"/>

    <ShellContent
        Title="SignIn"
        ContentTemplate="{DataTemplate local:SignInPage}"
        Route="SignIn" 
        Shell.FlyoutBehavior="Disabled"
        x:Name="SignInShell"/>

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage"
        x:Name="MainPageShell"/>

    <ShellContent
        Title="Register"
        ContentTemplate="{DataTemplate local:SignUpPage}"
        Route="Register" 
        x:Name="SignUpShell"/>


</Shell>
